<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.androidrssfeedtutorial">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        android:usesCleartextTraffic="true">
        <activity android:name="com.android.updatesInMedicine.Contact_us"></activity>
        <activity
            android:name="com.android.updatesInMedicine.JournalListActivity"
            android:theme="@android:style/Theme.Holo.Light.DarkActionBar"/>

        <activity android:name="com.android.updatesInMedicine.SubjectsList"
            android:theme="@android:style/Theme.Holo.Light.DarkActionBar"/>
        <activity android:name="com.android.updatesInMedicine.AboutActivity" />
        <activity android:name="com.android.updatesInMedicine.NewsActivity" />
        <activity android:name="com.android.updatesInMedicine.JournalActivity" />
        <activity android:name="com.android.updatesInMedicine.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.updatesInMedicine.BrowserActivity" />
        <activity android:name="com.android.updatesInMedicine.RSSFeedActivity"
            android:theme="@android:style/Theme.Holo.Light.DarkActionBar"/>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>