<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.androidrssfeedtutorial">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.android.updatesInMedicine.JournalListActivity"
            android:label="@string/title_activity_journal_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name="com.android.updatesInMedicine.SubjectsList" />
        <activity android:name="com.android.updatesInMedicine.AboutActivity" />
        <activity android:name="com.android.updatesInMedicine.NewsActivity" />
        <activity android:name="com.android.updatesInMedicine.JournalActivity" />
        <activity android:name="com.android.updatesInMedicine.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.updatesInMedicine.BrowserActivity" />
        <activity android:name="com.android.updatesInMedicine.RSSFeedActivity" />
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>